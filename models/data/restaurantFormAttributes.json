{
  "chinese": {
    "name": "店名",
    "name_en": "英文店名",
    "category": "類別",
    "image": "圖片網址",
    "location": "地址",
    "phone": "電話",
    "google_map": "google地圖連結",
    "rating": "評分",
    "description": "介紹"
  },
  "input": {
    "image": {
      "type": "url",
      "placeholder": "https 或 http 開頭",
      "pattern": "https?://(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{2,256}\\.[a-z]{2,6}\\b([-a-zA-Z0-9@:%_\\+.~#?&//=]*)",
      "title": "https 或 http 開頭"
    },
    "phone": {
      "type": "tel",
      "pattern": "(0\\d\\s\\d{3,4}\\s\\d{4})|(09\\d\\d\\s\\d{3}\\s\\d{3})",
      "placeholder": "市話: 0X XXXX XXXX 、手機: 09XX XXX XXX",
      "title": "市話: 0X XXXX XXXX 、手機: 09XX XXX XXX"
    },
    "google_map": {
      "type": "url",
      "placeholder": "https://goo.gl/maps/.....",
      "title": "https://goo.gl/maps/.....",
      "pattern": "https?://goo\\.gl/maps/[a-zA-Z0-9]+"
    },
    "rating": {
      "type": "number",
      "min": "0",
      "max": "5",
      "value": "0",
      "step": "0.1",
      "title": "請輸入0-5的數字，可到小數點下一位"
    },
    "description": {
      "rows": "4",
      "maxlength": "500",
      "placeholder": "上限500字"
    },
    "name": {
      "type": "text",
      "maxlength": "50",
      "placeholder": "上限50字",
      "pattern": ".*[^\\s]+.*"
    },
    "name_en": {
      "type": "text",
      "maxlength": "50",
      "placeholder": "上限50字"
    },
    "category": {
      "type": "text",
      "maxlength": "20",
      "placeholder": "上限20字",
      "pattern": ".*[^\\s]+.*"
    },
    "location": {
      "type": "text",
      "pattern": ".*[^\\s]+.*"
    }
  }
}